<!DOCTYPE html>
<html>
<head>
  <title>Password Protected Link</title>
  <script>
    function showLink() {
      var password = document.getElementById("password").value;
      var linkContainer = document.getElementById("link-container");
      if (password === atob("NTk2Mw==")) {
        var link = document.createElement("a");
        link.href = atob("aHR0cHM6Ly93d3c0LmhkLWV6LmNvbS9ocC90a3Ju");
        link.innerText = atob("5rKz5a2Q5pyo");
        linkContainer.appendChild(link);
      } else {
        linkContainer.innerHTML = atob("44K144Oz44Kv44K544Ov44O844Ot44Op44OJ44OX44Oz44O8IOOCquODvOODs+ODvOODh+ODvOODgeODvOODvOODvOODvA==");
      }
    }
  </script>
</head>
<body>
  <h1>パスワードがないと入れません。</h1>
  <input type="password" id="password" placeholder="パスワードを入力してください">
  <button onclick="showLink()">表示</button>
  <div id="link-container"></div>
</body>
</html>
